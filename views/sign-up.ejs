    <header>
      <!-- Create account section -->
      <section id="sign-up" class="hidden-dis dis-none">
        <div class="sign-up-section p-5">
          <div class="container">
            <div class="row">
              <div class="col-lg-5">
                <div class="sign-up-form p-4">
                  <div class="login-header">
                    <h3>
                      <span> <i class="fas fa-sign-in-alt"></i> </span>Create an
                      Account
                    </h3>
                  </div>
                  <form
                    action="/"
                    method="post"
                    enctype="multipart/form-data"
                    id="add-user-form"
                    class="form-group"
                  >
                    <div class="row gx-4 p-2">
                      <div class="col-6 p-2">
                        <label class="form-label fw-bold" for="firstname"
                          >First Name</label
                        >
                        <input
                          type="text"
                          class="form-control"
                          name="firstname"
                          placeholder="First Name"
                          required
                        />
                      </div>
                      <div class="col-6 p-2">
                        <label class="form-label fw-bold" for="lastname"
                          >Last Name</label
                        >
                        <input
                          type="text"
                          class="form-control"
                          placeholder="Last Name"
                          name="lastname"
                          id="lastname"
                          required
                        />
                      </div>
                      <div class="p-2">
                        <label class="form-label fw-bold" for="email"
                          >Enter Valid Email</label
                        >
                        <input
                          type="email"
                          class="form-control"
                          placeholder="Mobile or Email ID"
                          name="email"
                          id="email"
                          required
                        />
                      </div>
                      <div class="p-2">
                        <label class="form-label fw-bold" for="password"
                          >Enter Password</label
                        >
                        <input
                          type="password"
                          class="form-control"
                          placeholder="Password"
                          name="password"
                          id="password"
                          required
                        />
                        <label class="form-label fw-bold mt-4" for="avatar"
                          >Upload Profile Photo</label
                        >
                        <input
                          class="form-control"
                          type="file"
                          name="avatar"
                          id="avatar"
                        />
                      </div>
                    </div>
